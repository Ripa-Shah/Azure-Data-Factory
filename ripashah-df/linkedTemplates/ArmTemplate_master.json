{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ripashah-df"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"AzureBlobStorage3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage3'"
		},
		"AzureCallMultipleExcelSheet_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureCallMultipleExcelSheet'"
		},
		"Stock_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Stock'"
		},
		"StockEvaluation_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'StockEvaluation'"
		},
		"stockproject_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'stockproject'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ripashah-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorage2_connectionString": {
						"value": "[parameters('AzureBlobStorage2_connectionString')]"
					},
					"AzureBlobStorage3_connectionString": {
						"value": "[parameters('AzureBlobStorage3_connectionString')]"
					},
					"AzureCallMultipleExcelSheet_connectionString": {
						"value": "[parameters('AzureCallMultipleExcelSheet_connectionString')]"
					},
					"Stock_connectionString": {
						"value": "[parameters('Stock_connectionString')]"
					},
					"StockEvaluation_connectionString": {
						"value": "[parameters('StockEvaluation_connectionString')]"
					},
					"stockproject_connectionString": {
						"value": "[parameters('stockproject_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}