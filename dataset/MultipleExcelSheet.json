{
	"name": "MultipleExcelSheet",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"dsSheetName": {
				"type": "string",
				"defaultValue": "Apple Stocks"
			}
		},
		"annotations": [],
		"type": "Excel",
		"typeProperties": {
			"sheetName": {
				"value": "@dataset().dsSheetName",
				"type": "Expression"
			},
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": "stock_price_id.xlsx",
				"container": "azureblob"
			},
			"firstRowAsHeader": true
		},
		"schema": [
			{
				"name": "id",
				"type": "String"
			},
			{
				"name": "ticker",
				"type": "String"
			},
			{
				"name": "Date",
				"type": "String"
			},
			{
				"name": "Open",
				"type": "String"
			},
			{
				"name": "High",
				"type": "String"
			},
			{
				"name": "Low",
				"type": "String"
			},
			{
				"name": "Close",
				"type": "String"
			},
			{
				"name": "Volume",
				"type": "String"
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}